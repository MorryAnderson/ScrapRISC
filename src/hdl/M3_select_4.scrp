module M3_select_4 (
    input I_e1,
    input I_e2,
    input I_e3,
    input I_e4,
    input I_d1[32],
    input I_d2[32],
    input I_d3[32],
    input I_d4[32],
    output O_q[32]
);


gate a1 = and(I_d1, I_e1);
gate a2 = and(I_d2, I_e2);
gate a3 = and(I_d3, I_e3);
gate a4 = and(I_d4, I_e4);
gate q = or(a1,a2,a3,a4);
assign O_q = {q};


place a1 @(0,0,0);
place a2 @(1,0,0);
place a3 @(2,0,0);
place a4 @(3,0,0);
place q  @(4,0,0);


endmodule